<RelativeLayout 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingTop="5dp"
    android:paddingLeft="5dp"
    android:paddingRight="5dp">
    
     <LinearLayout
        android:id="@+id/btn_container"
        android:layout_width="fill_parent"
        android:layout_height="32dp"
        android:orientation="horizontal"
        android:gravity="center_vertical"
        android:background="#F5F5F5"
         >
        
        <Button
            android:id="@+id/all_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            style="@android:attr/buttonBarButtonStyle"
            android:textSize="12sp"
            android:background="@color/nav_bar_button"
            android:textColor="@color/nav_bar_text"
            android:text="@string/allcategory" />
         <Button
             android:id="@+id/gongbi_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="12sp"
            android:textColor="@color/nav_bar_text"
            android:background="@color/nav_bar_button"
            style="@android:attr/buttonBarButtonStyle"
            android:layout_weight="1"
            android:text="@string/gongbi" />
         
          <Button
              android:id="@+id/xieyi_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
             android:textSize="12sp"
             android:background="@color/nav_bar_button"
             android:textColor="@color/nav_bar_text"
             style="@android:attr/buttonBarButtonStyle"
             android:layout_weight="1"
            android:text="@string/xieyi" />
          
           <Button
               android:id="@+id/xihua_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
             android:textSize="12sp"
             android:background="@color/nav_bar_button"
             android:textColor="@color/nav_bar_text"
             style="@android:attr/buttonBarButtonStyle"
             android:layout_weight="1"
            android:text="@string/xihua" />
           
            <Button
                android:id="@+id/shufa_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
             android:textSize="12sp"
             android:textColor="@color/nav_bar_text"
             android:background="@color/nav_bar_button"
             style="@android:attr/buttonBarButtonStyle"
             android:layout_weight="1"
            android:text="@string/shufa" />
            
             <Button
                 android:id="@+id/other_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
             android:textSize="12sp"
             android:textColor="@color/nav_bar_text"
             android:background="@color/nav_bar_button"
             style="@android:attr/buttonBarButtonStyle"
             android:layout_weight="1"
            android:text="@string/others" />
             </LinearLayout>
             
              <LinearLayout 
             android:id="@+id/blank"
             android:layout_below="@id/btn_container"
             android:layout_height="4dp"
             android:background="@color/nav_bar_button"
             android:layout_width="match_parent"
             android:orientation="horizontal">
             <TextView 
                 android:id="@+id/allblank"
                 android:layout_height="match_parent"
                 android:layout_width="0dp"
                 android:layout_marginLeft="6dp"
                 android:layout_marginRight="6dp"
                 android:layout_weight="1"
                 />
             
             <TextView 
                 android:id="@+id/gongbiblank"
                 android:layout_height="match_parent"
                 android:layout_width="0dp"
                 android:layout_marginRight="6dp"
                 android:layout_marginLeft="6dp"
                 android:layout_weight="1"
                 />
             
              <TextView 
                 android:id="@+id/xieyiblank"
                 android:layout_height="match_parent"
                 android:layout_width="0dp"
                 android:layout_marginRight="6dp"
                 android:layout_marginLeft="6dp"
                 android:layout_weight="1"
                 />
              
               <TextView 
                 android:id="@+id/xihuablank"
                 android:layout_height="match_parent"
                android:layout_width="0dp"
                 android:layout_marginRight="6dp"
                 android:layout_marginLeft="6dp"
                 android:layout_weight="1"
                 />
               
                <TextView 
                 android:id="@+id/shufablank"
                 android:layout_height="match_parent"
                 android:layout_width="0dp"
                 android:layout_marginRight="6dp"
                 android:layout_marginLeft="6dp"
                 android:layout_weight="1"
                 />
                
                 <TextView 
                 android:id="@+id/otherblank"
                 android:layout_height="match_parent"
                 android:layout_width="0dp"
                 android:layout_marginRight="6dp"
                 android:layout_marginLeft="6dp"
                 android:layout_weight="1"
                 />
                 
         </LinearLayout>

    
 
    
<com.vgpt.androidpaintings.compoent.widget.AuctionScrollView
    android:id="@+id/auction_scroll_view"
    android:layout_width="match_parent"
    android:paddingTop="3dp"
    android:layout_height="match_parent" >
             
      
    <LinearLayout 
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        >
              
        <LinearLayout 
            android:baselineAligned="false"
            android:layout_width="match_parent"
            android:layout_height="18dp"
            android:orientation="horizontal"
            android:paddingLeft="3dp"
            android:paddingRight="3dp"
            >
            
            <LinearLayout 
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="match_parent"
                android:orientation="horizontal"
                >
        
        <TextView 
            android:layout_width="4dp"
            android:layout_height="match_parent"
            android:background="@drawable/retangle9"
            />
        <TextView 
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:text="@string/latest_auction"
            android:layout_gravity="left"
            android:textSize="@dimen/gallery_item_picname"/>
        </LinearLayout>
        
        
        <RelativeLayout 
            android:layout_weight="1"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            >
        
            <ImageView
            android:id="@+id/sjx"
            android:layout_height="13dp"
            android:layout_width="13dp"
            android:layout_centerVertical="true"
            android:scaleType="centerInside"
            android:src="@drawable/sjx"
            android:layout_alignParentRight="true"
            />
        
        <TextView 
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:layout_toLeftOf="@id/sjx"
            android:layout_centerVertical="true"
            android:text="@string/time_rank"
            android:textSize="@dimen/gallery_item_picname"/>
        
        
        </RelativeLayout>
        </LinearLayout>


    <LinearLayout
        android:baselineAligned="false"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal" >
                 
        

        <LinearLayout
            android:id="@+id/first_column"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="vertical" >
        </LinearLayout>

        <LinearLayout
            android:id="@+id/second_column"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="vertical" >
        </LinearLayout>

    </LinearLayout>
    </LinearLayout>

</com.vgpt.androidpaintings.compoent.widget.AuctionScrollView>
        

    
<TextView 
    android:id="@+id/loadmore"
    android:layout_alignParentBottom="true"
    android:layout_width="match_parent"
    android:layout_height="40dp"
    android:gravity="center"
    android:text="@string/loadmore_ing"
    android:background="#CFCFCF"
    android:visibility="invisible"
    android:textSize="25sp"/>
</RelativeLayout>